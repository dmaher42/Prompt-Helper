{
  "name": "prompt-helper-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts --format esm --out-dir dist --clean",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "prisma:generate": "prisma generate --schema src/db/schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema src/db/schema.prisma",
    "prisma:push": "prisma db push --schema src/db/schema.prisma",
    "prisma:seed": "tsx src/db/seeds.ts",
    "postinstall": "npm run prisma:generate"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pino": "^8.17.0",
    "pino-http": "^9.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.12.7",
    "@types/supertest": "^2.0.16",
    "eslint": "^9.5.0",
    "eslint-plugin-security": "^2.1.1",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.12.1",
    "supertest": "^6.4.2",
    "tsup": "^8.0.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
